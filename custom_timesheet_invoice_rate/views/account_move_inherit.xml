<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Add contract_rate into invoice_line_ids-->
    <record model="ir.ui.view" id="view_invoice_contract_rate_form">
        <field name="name">account.invoice.contract.rate.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page/field[@name='invoice_line_ids']/tree/field[@name='quantity']" position="before">
                <field name="contract_rate"/>
                <field name="working_day"/>
            </xpath>
            <xpath expr="//page/field[@name='invoice_line_ids']/tree/field[@name='quantity']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <!--<xpath expr="//page/field[@name='invoice_line_ids']/tree/field[@name='quantity']" position="attributes">
               <attribute name="string">Working Day(Hrs/Days)</attribute>
            </xpath>-->
        </field>
    </record>


</odoo>
