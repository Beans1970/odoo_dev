<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
            <record id="hr_timesheet_sheet_inherited_view" model="ir.ui.view">
                <field name="name">hr.timesheet.sheet.tree.inherited</field>
                <field name="model">hr_timesheet.sheet</field>
                <field name="inherit_id" ref="hr_timesheet_sheet.hr_timesheet_sheet_tree"/>
                <field name="arch" type="xml">
                    <xpath expr="//field[@name='employee_id']" position="after">
                        <field name="timesheet_invoice_id" widget="many2onebutton"/>
                    </xpath>
                </field>
            </record>

    </data>
</odoo>